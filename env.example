# ============================================
# AI PALM READER - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# OpenRouter API Configuration
# Get your API key from: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# API Base URL (do not change unless necessary)
VITE_API_BASE_URL=https://openrouter.ai/api/v1

# AI Models Configuration
VITE_VISION_MODEL=allenai/molmo-2-8b:free
VITE_TEXT_MODEL=liquid/lfm-2.5-1.2b-thinking:free

# App Configuration
VITE_APP_NAME=AI Palm Reader
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://aipalmreader.app

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA_INSTALL_PROMPT=true

# Debug Mode (set to false in production)
VITE_DEBUG_MODE=false
